olors: [],\n  };\n\n  componentDidMount() {\n    this._interval = setInterval(() => {\n      this.setState({\n        colors: this.props.children.split('').map((_) => {\n          return randomColor();\n        }),\n      });\n    }, 1000);\n  }\n  componentWillUnmount() {\n    clearInterval(this._interval);\n  }\n\n  render() {\n    return this.props.children.split('').map((char, index) => (\n      <span key={index} style={{ color: this.state.colors[index] }}>\n        {char}\n      </span>\n    ));\n  }\n}\n\nexport default RandomColorText;\n","/Users/yuhanlin/Desktop/SEP-Training/React/todo-manager/src/utils/randomColor.util.js",[],"/Users/yuhanlin/Desktop/SEP-Training/React/todo-manager/src/components/ColorTodo/ColorTodo.js",["92","93"],"import React from 'react';\nimport RandomColorText from '../RandomColorText/RandomColorText';\nimport { limitCharStr } from '../../utils/limitCharStr.util';\n\nimport './ColorTodo.css';\n\nclass ColorTodo extends React.Component {\n  render() {\n    const { id, userId, completed, title } = this.props.todo;\n    const { handleRemoveTodo } = this.props;\n    return (\n      <li className=\"todo\">\n        <p className=\"todo__content\">\n          <RandomColorText>{limitCharStr(title, 40, '...')}</RandomColorText>\n        </p>\n        <button\n          type=\"button\"\n          className=\"todo__btn-remove\"\n          onClick={() => handleRemoveTodo(id)}\n        >\n          X\n        </button>\n      </li>\n    );\n  }\n}\nexport default ColorTodo;\n","/Users/yuhanlin/Desktop/SEP-Training/React/todo-manager/src/components/Counter/Counter.js",["94"],"/Users/yuhanlin/Desktop/SEP-Training/React/todo-manager/src/Redux/Redux.js",["95"],"import { createStore } from 'redux';\n// const { createStore } = require('redux');\n\n// Action Types\nconst INCREMENTED = 'counter/incremented';\nconst DECREMENTED = 'counter/decremented';\nconst actionTypes = {\n  INCREMENTED,\n  DECREMENTED,\n};\n\n// Action Creator\nconst incremented = () =